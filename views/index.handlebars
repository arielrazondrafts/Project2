<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>

        <h2>Outstanding Invoices</h2>
<table>
    <tr>
      <th>Invoice Amount</th>
      <th>Employer Name</th>
    </tr>
  {{#each invoices}}
      
       <tr><td>{{this.invoiceAmount}}</td>
       <td>{{this.Employer.employerName}}</td>
       <td>{{this.paidStatus}}</td>
       <td>{{this.EmployerId}}</td>
       <td><button class="paid" data-id="{{this.EmployerId}}">Paid</button></td>
       <td><button class="remind">Remind</button></td>
       </tr>
  {{/each}}
</table>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>
<script src="/js/invoices.js"></script>